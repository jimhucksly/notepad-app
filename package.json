{
  "name": "notepad.j-studio.app",
  "version": "1.0.0",
  "author": "Jimhucksly Studio <admin@jimhucksly-studio.ru>",
  "url": "https://jimhucksly-studio.ru",
  "description": "An electron-vue project",
  "license": "Jimhucksly Studio <admin@jimhucksly-studio.ru>",
  "main": "./dist/electron/main.js",
  "scripts": {
    "dev": "node .electron-vue/dev-runner.js",
    "build": "node .electron-vue/build.js && electron-builder -w",
    "e2e": "npm run pack && mocha test/e2e",
    "test": "npm run unit && npm run e2e",
    "unit": "jest"
  },
  "build": {
    "productName": "Notepad App",
    "appId": "com.example.notepad-jimhucksly-studioApp",
    "copyright": "Copyright Â© 2019-2020 ${author}",
    "directories": {
      "output": "build"
    },
    "files": [
      "**/*",
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "static/icon.ico",
      "target": [
        "nsis"
      ]
    },
    "squirrelWindows": {
      "iconUrl": "https://raw.githubusercontent.com/matiastucci/electron-auto-updater-example/master/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "jest": {
    "preset": "ts-jest",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    }
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@types/auto-launch": "^5.0.1",
    "@types/jest": "^25.1.2",
    "@types/lodash": "^4.14.149",
    "@types/markdown-it": "^0.0.9",
    "@types/markdown-it-anchor": "^4.0.3",
    "@types/simplemde": "^1.11.7",
    "@types/vue": "^2.0.0",
    "animejs": "^3.1.0",
    "auto-updater": "^1.0.2",
    "axios": "^0.19.2",
    "electron-updater": "^4.0.14",
    "express": "^4.17.1",
    "fs": "^0.0.1-security",
    "json-formatter-js": "^2.3.3",
    "markdown-it": "^10.0.0",
    "markdown-it-anchor": "^5.2.5",
    "minimatch": "^3.0.4",
    "run-electron": "^1.0.0",
    "simplemde": "^1.11.2",
    "vue": "^2.5.16",
    "vue-electron": "^1.0.6",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-electron": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "@vue/test-utils": "^1.0.3",
    "ajv": "^6.12.3",
    "auto-launch": "^5.0.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-register": "^6.26.0",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^2.1.2",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "8.3.4",
    "electron-builder": "^20.19.2",
    "electron-debug": "^3.0.1",
    "electron-squirrel-startup": "^1.0.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-import": "^2.12.0",
